# Identity
APP_ID=student-app

# SQS
AWS_REGION=eu-central-1
SQS_QUEUE_URL=https://sqs.eu-central-1.amazonaws.com/123456789012/ml-lab-dev-work
SQS_RECEIVE_WAIT_SECONDS=10
SQS_MAX_BATCH=10
SQS_VISIBILITY_TIMEOUT_SECONDS=60

# Validator
# Use infra host private IP (yours: 172.31.15.193)
VALIDATOR_URL=http://172.31.15.193:9200
VALIDATOR_TIMEOUT_SECONDS=3.0
VALIDATOR_CLIENT=requests  # requests|httpx

# CPU simulation
CPU_MS_PER_MESSAGE=0

# Metrics
METRICS_BIND=127.0.0.1
METRICS_PORT=9301

# Behaviour
DELETE_ON_4XX=true
DELETE_ON_5XX=false
LOG_LEVEL=INFO
